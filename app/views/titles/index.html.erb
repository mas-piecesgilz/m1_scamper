<!-- Box -->
<div class="box">
	<!-- Box Head -->
	<div class="box-head">
		<h2 class="left">Lists of Titles</h2>
	</div>
	<div class="tlist">
	    <table width=90% border="0" cellpadding="0" cellspacing="0">
		  <tr>
    <th>Title Code</th>
    <th>Name</th>
    <th>Berhormat</th>
    <th colspan=3 class="ac">Control</th>
  </tr>

<% @titles.each do |title| %>
  <tr>
    <td><%=h title.titlecode %></td>
    <td><%=h title.name %></td>
    <td><%=h title.berhormat %></td>
    <td><%= link_to 'Show', title %></td>
    <td><%= link_to 'Edit', edit_title_path(title) %></td>
    <td><%= link_to 'Destroy', title, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Title', new_title_path %>