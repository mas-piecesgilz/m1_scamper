<div class="tform">
<table width=100%>
	<tr>
		<th colspan=2>New Period Timing</td>
	</tr>
	
	<tr>
		<td class="leftcol"><label for="intake_id">Intake Session:</label></td>
		<td><%= f.collection_select :intake_id, Intake.find(:all), :id, :name, :include_blank => true %></td>
	</tr>
	
	<tr>
		<td class="leftcol"><label for="name">Name:</label></td>
		<td><%= f.text_field :name %></td>
	</tr>
	
	<tr>
		<td class="leftcol"><label for="start_time">Start Time:</label></td>
		<td><%= f.time_select :start_time, :minute_step => 5%></td>
	</tr>
	
	<tr>
		<td class="leftcol"><label for="end_time">End Time:</label></td>
		<td><%= f.time_select :end_time, :minute_step => 5%></td>
	</tr>
	
	<tr>
		<td class="leftcol"><label for="break">Break:</label></td>
		<td><%= f.check_box :break %></td>
	</tr>
	
	
	
</table>
<BR>